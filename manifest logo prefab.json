{
    "manifest_version": 3,
    "name": "Pace",
    "description": "Track your files!",
    "version": "0.01",

    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "storage"
    ]
}